namespace ul http://underlay.org/ns/

type value [
	ul:uri
	ul:literal <- !
	ul:product <- * ul:product
	ul:coproduct <- * ul:coproduct
	ul:reference <- * ul:class
]

class ul:product {}

class ul:component {
	ul:source -> * ul:product
	ul:key -> !
	ul:value -> value
}

class ul:coproduct {}

class ul:option {
	ul:source -> * ul:coproduct
	ul:key -> !
	ul:value -> value
}

class ul:class {
	ul:key -> !
	ul:value -> value
}